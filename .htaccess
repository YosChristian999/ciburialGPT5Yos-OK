Options -Indexes
ServerSignature Off

<IfModule mod_rewrite.c>
  RewriteEngine On
</IfModule>
<IfModule mod_headers.c>
  Header always set Content-Security-Policy "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net https://unpkg.com https://app.sandbox.midtrans.com https://app.midtrans.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdn.jsdelivr.net; img-src 'self' data: blob: https:; font-src 'self' data: https://fonts.gstatic.com; connect-src 'self' https://api.sandbox.midtrans.com https://api.midtrans.com https://app.sandbox.midtrans.com https://app.midtrans.com https://cdn.jsdelivr.net; frame-src https://app.sandbox.midtrans.com https://app.midtrans.com; base-uri 'self'; form-action 'self'; object-src 'none'"
</IfModule>
